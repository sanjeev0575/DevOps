{
  "family": "python-app-task",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::115456585578:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "python-app",
      "image": "115456585578.dkr.ecr.us-east-1.amazonaws.com/devops:39",
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
